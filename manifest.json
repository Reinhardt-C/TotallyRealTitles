{
    "manifest_version": 3,
    "name": "TotallyRealTitles",
    "description": "Adds more titles to Lichess",
    "version": "1.0",
    "action": {
        "default_popup": "./popup/index.html",
        "default_icon": "./assets/lm.png"
    },
    "content_scripts": [
        {
            "matches": ["http://lichess.org/*", "https://lichess.org/*"],
            "js": ["./scripts/main.js"]
        }
    ]
}
